# OpenAI 직접 사용 예시:
# SUMMARIZATION_MODEL=gpt-4o-mini
# RESEARCH_MODEL=gpt-4o
# COMPRESSION_MODEL=gpt-4o-mini
# FINAL_REPORT_MODEL=gpt-4o

#서치 툴 
SEARCH_APIS=tavily,duckduckgo


#리서처 옵션
MAX_CONCURRENT_RESEARCH_UNITS=5
